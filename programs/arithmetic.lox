// Basic arithmetic operations
print "=== Arithmetic Tests ===";

// Addition
print 1 + 2;
print 3.14 + 2.86;

// Subtraction
print 10 - 5;
print 7.5 - 2.3;

// Multiplication
print 3 * 4;
print 2.5 * 3.2;

// Division
print 12 / 3;
print 15.6 / 2.4;

// Precedence
print 2 + 3 * 4;
print (2 + 3) * 4;
print 12 / 3 + 2;
print 12 / (3 + 2);

// Unary minus
print -5;
print -(3 + 4);